{% extends "layout.twig" %}

{% block body %}
<h3 class="mt-5 mb-5">{{ pageTitle }}</h3>

<table class="table table-striped table-bordered">
    <th>{{ "Date"|xlt }}</th>
    <th>{{ "Sender"|xlt }}</th>
    <th>{{ "Number"|xlt }}</th>
    <th>{{ "Time"|xlt }}</th>
    <th>{{ "Page Count"|xlt }}</th>
    <th>{{ "Status"|xlt }}</th>
    <th>{{ "Details"|xlt }}</th>

    {% for entries in data %}
        {{ entries }}
        {% for key, value in entries %}
            - {{ key }}
        <tr>
            <td> - {{ value.createdAt|e }}1</td>
            <td> - {{ value.users|e }}2</td>
            <td> - {{ value.faxNumber|e }}3</td>
            <td> - {{ value.direction|xlt }}4</td>
            <td> - {{ value.pagesCount|e }}5</td>
            <td> - {{ value.processingStatusName|xlt }}6</td>
            <td> - {{ value.resultInfo|xlt }}7</td>
        </tr>
            {% endfor %}
    {% endfor %}
</table>
{% endblock %}
