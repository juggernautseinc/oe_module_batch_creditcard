{% extends "layout.twig" %}

{% block body %}
<h3 class="mt-5 mb-5">{{ pageTitle }}</h3>

<table class="table table-striped table-bordered">
    <th>{{ "Date"|xlt }}</th>
    <th>{{ "Sender"|xlt }}</th>
    <th>{{ "Number"|xlt }}</th>
    <th>{{ "Time"|xlt }}</th>
    <th>{{ "Page Count"|xlt }}</th>
    <th>{{ "Status"|xlt }}</th>
    <th>{{ "Details"|xlt }}</th>

    {% for entries in data %}
        {{ entries }}
        {% for key, value in entries %}
        <tr>
            <td>{{ value.createdAt|e }}</td>
            <td>{{ value.users|e }}</td>
            <td>{{ value.faxNumber|e }}</td>
            <td>{{ value.direction|xlt }}</td>
            <td>{{ value.pagesCount|e }}</td>
            <td>{{ value.processingStatusName|xlt }}</td>
            <td>{{ value.resultInfo|xlt }}</td>
        </tr>
            {% endfor %}
    {% endfor %}
</table>
{% endblock %}
