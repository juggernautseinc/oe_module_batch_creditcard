{% extends "layout.twig" %}

{% block body %}
<h3 class="mt-5 mb-5">{{ pageTitle }}</h3>

<table class="table table-striped table-bordered">
    <th>{{ "Date"|xlt }}</th>
    <th>{{ "Sender"|xlt }}</th>
    <th>{{ "Number"|xlt }}</th>
    <th>{{ "Time"|xlt }}</th>
    <th>{{ "Page Count"|xlt }}</th>
    <th>{{ "Status"|xlt }}</th>
    <th>{{ "Details"|xlt }}</th>

    {% for entry in data %}
        <tr>
            <td>{{ entry.createdAt }}</td>
            <td>{{ entry.users }}</td>
            <td>{{ entry.faxNumber }}</td>
            <td>{{ entry.direction|xlt }}</td>
            <td>{{ entry.pagesCount }}</td>
            <td>{{ entry.processingStatusName|xlt }}</td>
            <td>{{ entry.resultInfo|xlt }}</td>
        </tr>
    {% endfor %}
</table>
{% endblock %}
