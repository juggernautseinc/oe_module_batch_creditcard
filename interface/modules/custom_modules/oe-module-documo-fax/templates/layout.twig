<!doctype html>
<html lang="en">
<head>

    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>{{ pageTitle }}</title>
    {{ header }}
    {%  block head %}{% endblock %}
</head>
<body>
<div id="body" class="container">
    {%  block body %}
    {% endblock %}

    {% block footer %}
    {% endblock %}
    <div class="mt-10">
        &copy; {{ 'now'|date('Y') }}
    </div>
</div>

</body>
</html>
